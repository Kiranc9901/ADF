{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADEKC12"
		},
		"ADLSlinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSlinkedService'"
		},
		"Landing_Container_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Landing_Container_LS'"
		},
		"SQL_DB_parameterised_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQL_DB_parameterised_LS'"
		},
		"SqldbNameService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqldbNameService'"
		},
		"ADLSlinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kirancadls.dfs.core.windows.net/"
		},
		"Landing_Container_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kiranc30062000adls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADEKC12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSlinkedService_accountKey": {
						"value": "[parameters('ADLSlinkedService_accountKey')]"
					},
					"Landing_Container_LS_accountKey": {
						"value": "[parameters('Landing_Container_LS_accountKey')]"
					},
					"SQL_DB_parameterised_LS_connectionString": {
						"value": "[parameters('SQL_DB_parameterised_LS_connectionString')]"
					},
					"SqldbNameService_connectionString": {
						"value": "[parameters('SqldbNameService_connectionString')]"
					},
					"ADLSlinkedService_properties_typeProperties_url": {
						"value": "[parameters('ADLSlinkedService_properties_typeProperties_url')]"
					},
					"Landing_Container_LS_properties_typeProperties_url": {
						"value": "[parameters('Landing_Container_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADEKC12_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADEKC12_ArmTemplate_0')]"
			]
		}
	]
}